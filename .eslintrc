{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": false
  },
  "rules": {
    "no-undef": "off",
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "semi": "off",
    "require-jsdoc": "off",
    "indent": ["error", 2]
  },
  "extends": ["eslint:recommended", "google"]
}
